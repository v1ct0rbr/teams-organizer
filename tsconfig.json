{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["styled-components/native"],
    "strict": true,
    "paths": {
      "@assets/*" : [
        "./src/assets/*"
      ],
      "@components/*" : [
        "./src/components/*"
      ],
      "@routes/*" : [
        "./src/routes/*"
      ],
      "@screens/*" : [
        "./src/screens/*"
      ],
      "@storage/*" : [
        "./src/storage/*"
      ],
      "@utils/*" : [
        "./src/utils/*"
      ],
    },
    
  }
}
